
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0,viewport-fit=cover">
    <title>错误</title>
    <link href="~/lib/weui/dist/style/weui.min.css" rel="stylesheet" />
</head>
<body ontouchstart>
    <div class="page msg_warn js_show">
        <div class="weui-msg">
            <div class="weui-msg__icon-area"><i class="weui-icon-warn weui-icon_msg"></i></div>
            <div class="weui-msg__text-area">
                <h2 class="weui-msg__title">@ViewData["Title"]</h2>
                <p class="weui-msg__desc">@ViewData["Message"]</p>
            </div>
            <div class="weui-msg__opr-area">
                <p class="weui-btn-area">
                    <a href="/ServiceApply/Index" class="weui-btn weui-btn_primary">返回列表</a>
                    <a href="javascript:history.back();" class="weui-btn weui-btn_default">返回上一页</a>
                </p>
            </div>
        </div>
    </div>
</body>
</html>
