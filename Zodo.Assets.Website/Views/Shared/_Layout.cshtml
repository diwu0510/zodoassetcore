<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/css/myui.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/myui.css" asp-append-version="true" />
    </environment>
    <link href="~/lib/viewer/dist/viewer.css" rel="stylesheet" />
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()

    <environment include="Development">
        <script src="~/lib/jquery/jquery.min.js"></script>
        <script src="~/lib/layer/layer.js"></script>
        <script src="~/lib/myui/myui.v2.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/jquery.min.js"></script>
        <script src="~/lib/layer/layer.js"></script>
        <script src="~/lib/myui/myui.v2.js"></script>
    </environment>
    <script src="~/lib/viewer/dist/viewer.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
